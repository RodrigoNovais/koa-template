        "@types/koa": "^2.11.3",
        "@types/koa-bodyparser": "^4.3.0",
        "@types/koa-router": "^7.4.1",
        "@types/koa2-cors": "^2.0.1",
        "@typescript-eslint/eslint-plugin": "^2.34.0",
        "@typescript-eslint/parser": "^2.34.0",
        "dotenv": "^8.2.0",
        "eslint": "^6.8.0",
        "eslint-plugin-security": "^1.4.0",
        "jest": "^25.5.4",
        "ts-jest": "^25.5.1",
        "ts-node-dev": "^1.0.0",
        "typescript": "^3.9.7"
    },
